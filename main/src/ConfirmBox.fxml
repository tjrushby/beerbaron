<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@popup-box.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ConfirmBoxController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" styleClass="vbox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="labelMessage" text="Message" />
            <HBox alignment="CENTER" styleClass="hbox">
               <children>
                  <Button fx:id="buttonYes" mnemonicParsing="false" text="Yes" />
                  <Button fx:id="buttonNo" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="No" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
